{
    "name": "dms/package.faqs",
    "description": "The generic FAQ's package for the DMS",
    "authors": [
        {
            "name": "Elliot Levin",
            "email": "elliotlevin@hotmail.com"
        }
    ],
    "minimum-stability": "stable",
    "repositories": [
        {"type": "composer", "url": "https://api.package.studio/meta/composer"}
    ],
    "autoload": {
        "psr-4": {
            "Dms\\Package\\Faq\\": "./src/"
        }
    },
    "require": {
        "php": ">=7.0.0",
        "dms/core": "dev-master",
        "dms/common.structure": "dev-master"
    },
    "autoload-dev": {
        "psr-4": {
            "Dms\\Package\\Package\\Faq\\Tests\\": "./tests/Tests/",
            "Dms\\Core\\Tests\\": "./vendor/dms/core/tests/Tests/"
        }
    },
    "require-dev": {
        "dms/common.testing": "dev-master"
    }
}
